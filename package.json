{
  "name": "rivet-icons",
  "version": "0.6.0",
  "description": "Icon set for Indiana University's Rivet design system",
  "files": [
    "dist/**/*"
  ],
  "main": "index.js",
  "scripts": {
    "build": "run-s build-icons build-docs",
    "build-docs": "npx eleventy",
