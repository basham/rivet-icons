  "bugs": {
    "url": "https://github.com/indiana-university/rivet-icons/issues"
  },
  "homepage": "https://github.com/indiana-university/rivet-icons#readme",
  "devDependencies": {
    "@11ty/eleventy": "0.12.1",
    "gh-pages": "3.1.0",
    "npm-run-all": "4.1.5",
    "path-that-svg": "1.2.4",
    "svg-sprite": "1.5.0"
  }
}
